    "import numpy as np\n",
    "\n",
    "\n",
    "lat_km = 92\n",
    "lng_km = 111\n",
    "\n",
    "\n",
    "def calc_distance(lat1, lng1, lat2, lng2):\n",
    "    \"\"\"Return Euclidean distance in kilometers.\"\"\"\n",
    "    delta_lat = (lat1 - lat2) * lat_km\n",
    "    delta_lng = (lng1 - lng2) * lng_km\n",
    "    return np.hypot(delta_lat, delta_lng)"
   ]
